  },
  "scripts": {
    "start": "npm run watch:css && react-app-rewired start",
    "build": "npm run build:css && react-scripts build  ",
    "reduce-css": "purgecss --css build/static/css/*.*.css --content build/index.html build/static/js/*.js --output build/static/css",
    "eject": "react-scripts eject",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/main.css",
    "watch:css": "postcss src/styles/tailwind.css -o src/styles/main.css",
    "test-coverage": "react-scripts test --silent --coverage --watchAll=false",
    "test": "react-scripts test --verbose"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "customize-cra": "^1.0.0",
    "customize-cra-react-refresh": "^1.1.0",
    "kcd-scripts": "^5.9.0",
    "react-app-rewired": "^2.1.8"
  }
