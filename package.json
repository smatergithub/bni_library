{
  "name": "bni_library",
  "version": "1.0.0",
  "description": "E-Learning",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server/index.js",
    "build-prod": "npm run build",
    "build-prod-win": "npm run winBuild",
    "setup-dev": "concurrently \"sequelize db:migrate \" \"cd react && npm install\"",
    "setup-prod": "concurrently \"sequelize db:migrate --env production \" \"cd react && npm install\"",
    "clean": "rm -rf react/build",
    "build": "npm run clean && cd react && INLINE_RUNTIME_CHUNK=false GENERATE_SOURCEMAP=false && npm run build",
    "winBuild": "npm run clean && cd react && set \"GENERATE_SOURCEMAP=false\"  \"INLINE_RUNTIME_CHUNK=false\" && npm run build",
    "dev-react": "cd react && npm run start",
    "dev-server": "cross-env NODE_ENV=development nodemon --watch server server/index.js",
    "prod-server": "cross-env NODE_ENV=production nodemon --watch server server/index.js",
    "dev": "concurrently \"npm:dev-server \" \"npm:dev-react\"",
    "bni-library": "concurrently \"npm:dev-server \" \"npm:dev-react\""
  },
  "keywords": [
    "bni"
  ],
  "author": "nagacoder",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "crypto-random-string": "^3.2.0",
    "dotenv": "^8.2.0",
    "excel4node": "^1.7.2",
    "express": "^4.17.1",
    "formidable": "^1.2.2",
    "fs": "0.0.1-security",
    "https": "^1.0.0",
    "husky": "2.2.0",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^2.5.2",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mustache": "^4.0.1",
    "mysql2": "^2.1.0",
    "node-cmd": "^3.0.0",
    "node-excel-export": "^1.4.4",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.14.1",
    "nodemailer": "^6.4.16",
    "nodemon": "^2.0.3",
    "path": "^0.12.7",
    "pdf-lib": "^1.11.1",
    "pretty-quick": "^2.0.1",
    "prop-types": "^15.7.2",
    "read-excel-file": "^4.0.6",
    "sequelize": "^5.21.7",
    "sweetalert": "^2.1.2",
    "uuid": "^8.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "eslint-plugin-flowtype": "^4.7.0",
    "sequelize-cli": "^6.2.0"
  }
}
